    "    index = random.randint(0,n-1)\n",
    "    currConflicts = conflicts(board)\n",
    "    temp = board.copy()\n",
    "    count = 0\n",
    "    while len(theMove) <= 0:\n",
    "        for x in range(n):\n",
    "            if board[index] != x:\n",
    "                temp[index] = x\n",
    "                if conflicts(temp) < currConflicts:\n",
    "                    theMove = [index, x]\n",
    "        index = random.randint(0,n-1)\n",
    "        count += 1\n",
    "        if count > 1000:\n",
    "            return theMove\n",
    "\n",
